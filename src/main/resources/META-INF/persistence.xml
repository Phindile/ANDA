  <persistence-unit name="com.phindile_Atmweb_war_1.0-SNAPSHOTPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.phindile.atmweb.domain.Log</class>
